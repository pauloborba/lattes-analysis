	<queryString>
		<![CDATA[SELECT
    b.TipoOrientacao,b.titulo, b.ano, b.natureza, c.name, b.nomeOrientando,
    (SELECT
   count(b.titulo)
FROM
    lattes.orientation b
        INNER JOIN
    lattes.researcher_orientation co ON b.id = co.orientations_id
        INNER JOIN
    lattes.researcher c ON c.id = co.researcher_id
WHERE
	b.TipoOrientacao = 'ORIENTACAO_INICIACAO_CIENTIFICA_ANDAMENTO') as 'total'
FROM
    lattes.orientation b
        INNER JOIN
    lattes.researcher_orientation co ON b.id = co.orientations_id
        INNER JOIN
    lattes.researcher c ON c.id = co.researcher_id
WHERE
	b.TipoOrientacao = 'ORIENTACAO_INICIACAO_CIENTIFICA_ANDAMENTO'

ORDER BY b.ano ASC;]]>
	</queryString>
